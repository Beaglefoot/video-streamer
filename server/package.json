{
  "name": "server",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist/*",
    "lint": "eslint src/**/*.ts"
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,js}": [
      "eslint",
      "git add"
    ]
  },
  "dependencies": {
    "express": "4.17.1"
  },
  "devDependencies": {
    "@types/express": "4.17.6",
    "@types/node": "13.13.4",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "lint-staged": "10.2.2",
    "prettier": "2.0.5",
    "typescript": "3.8.3"
  }
}
